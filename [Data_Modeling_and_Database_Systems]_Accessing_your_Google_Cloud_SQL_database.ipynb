      ],
      "metadata": {
        "id": "PBIXW4VvxMl4"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "with engine.connect() as db_conn:\n",
        "  results = db_conn.execute(sqlalchemy.text(\"SELECT * FROM students\")).fetchall()\n",
        "\n",
        "  for row in results:\n",
        "    print(row)"
